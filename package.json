{
  "name": "@beqom/data-grid",
  "version": "0.0.3",
  "main": "index.js",
  "repository": "https://github.com/beqom/data-grid.git",
  "author": "Renaud Tertrais <renaud.tertrais@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint:style": "stylelint \"src/*.scss\" \"src/**/*.scss\" \"src/*.css\" \"src/**/*.css\"",
    "lint:script": "eslint .",
    "lint": "npm run lint:script && npm run lint:style",
    "clean:storybook": "rimraf build/storybook",
    "prebuild:storybook": "npm run clean:storybook",
    "build:storybook": "build-storybook -c .storybook -o build/storybook",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "predeploy": "npm run build:storybook",
    "deploy": "gh-pages -d build/storybook",
    "clean:dist": "rimraf dist",
    "prebuild": "npm run clean:dist && copy ./package.json ./README.md ./dist",
    "build": "cross-env NODE_ENV=production babel ./src -d ./dist --ignore story.js,__tests__,data.json --copy-files",
    "start": "npm run storybook"
  },
  "devDependencies": {
    "@beqom/alto-ui": "0.3.2",
    "@storybook/addon-centered": "3.2.16",
